[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[packages]
requests = "*"
googlemaps = "*"
fastapi = "*"
uvicorn = "*"
pydantic = "*"
responses = "*"
flake8 = "*"

[requires]
python_version = "3.9"

[scripts]
dev = "uvicorn app.main:app --reload"
serve = "uvicorn app.main:app --host 0.0.0.0 --port 80"
test = "python3 -m unittest discover -v app/tests"

[dev-packages]
black = "*"
rope = "*"
uvicorn = "*"

[pipenv]
allow_prereleases = true
