    "eslint-plugin-prettier": "^3.4.1",
    "gmap-vue": "^1.5.0",
    "postcss": "^7.0.39",
    "prettier": "^2.7.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "vue": "^2.7.8",
    "vue-i18n": "^8.27.2",
    "vue-material-design-icons": "^4.13.0",
    "vuex": "^3.6.2"
